{
  "name": "webpack-sample-project",
  "version": "1.1.0",
  "description": "PWA",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --progress",
    "build": "NODE_ENV=production node_modules/.bin/webpack --config ./webpack.production.config.js --progress",
    "winbuild": "set NODE_ENV=production&& webpack -p --config ./webpack.production.config.js --progress",
    "test": "jest"
  },
  "author": "author",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "~6.7.*",
    "babel-loader": "~6.2.*",
    "babel-preset-es2015": "~6.6.*",
    "babel-preset-react": "~6.5.*",
    "css-loader": "^0.28.7",
    "esformatter-jsx": "^8.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.15.0",
    "jest": "^20.0.4",
    "json-loader": "^0.5.7",
    "postcss-loader": "^0.8.2",
    "redux-devtools": "^3.4.0",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "dexie": "^1.5.1",
    "immutability-helper": "^2.3.1",
    "prop-types": "^15.6.0",
    "rc-queue-anim": "^1.3.0",
    "rc-tween-one": "^1.5.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-textarea-autosize": "^5.1.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "esformatter": {
    "plugins": [
      "esformatter-jsx"
    ],
    "jsx": {
      "formatJSXExpressions": true,
      "JSXExpressionsSingleLine": true,
      "formatJSX": true,
      "attrsOnSameLineAsTag": true,
      "maxAttrsOnTag": 1,
      "firstAttributeOnSameLine": false,
      "spaceInJSXExpressionContainers": " ",
      "alignWithFirstAttribute": true,
      "htmlOptions": {
        "brace_style": "collapse",
        "indent_char": " ",
        "indent_size": 2,
        "max_preserve_newlines": 2,
        "preserve_newlines": true
      }
    }
  }
}
